<h1>Listing blocked_users</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @blocked_users.each do |blocked_user| %>
      <tr>
        <td><%= link_to 'Show', blocked_user %></td>
        <td><%= link_to 'Edit', edit_blocked_user_path(blocked_user) %></td>
        <td><%= link_to 'Destroy', blocked_user, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Blocked user', new_blocked_user_path %>
